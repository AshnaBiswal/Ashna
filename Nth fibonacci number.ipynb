    "#python function to return the nth fibonacci number.\n",
    "def fib(n):\n",
    "    prev=0\n",
    "    cur=1\n",
    "    for i in range(n-2):\n",
    "        next=cur+prev\n",
    "        prev=cur\n",
    "        cur=next\n",
    "    print(n,\"th fibonacci number is\",next,end=\"\")    \n",
    "fib(10)        "
   ]
  }
